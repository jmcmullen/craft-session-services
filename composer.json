{
  "name": "jmcmullen/session-services",
  "description": "Session Services integration for Craft CMS",
  "type": "craft-plugin",
  "require": {
    "php": ">=8.0.2",
    "craftcms/cms": "^4.5.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "jmcmullen\\sessionservices\\": "src/"
    }
  },
  "extra": {
    "handle": "_session-services",
    "name": "Session Services",
    "developer": "Jay McMullen",
    "documentationUrl": "",
    "class": "jmcmullen\\sessionservices\\SessionServices"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
